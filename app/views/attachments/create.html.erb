<h1>Attachments#create</h1>
<%= form_for(:attachments, :url => {:action => 'create'},:html => {:multipart => true}) do %>
    <%= file_field_tag 'attachment' %>
    <%= submit_tag "Send Attachment" %>
<p>Done? <%= link_to "Sign out", session_path(current_user.id), :method => :delete %></p>
<% end %>
